{
  "name": "kadukadu",
  "version": "1.0.0",
  "description": "Akkadu's popover dictionary for language learning",
  "main": "index.js",
  "repository": "https://github.com/Akkadu/kadukadu",
  "author": "codebender828 <jonas@akkadu-team.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "concurrently yarn:build:*",
    "dev": "concurrently yarn:dev:*",
    "dev:build": "cross-env swc src --out-dir dist/esm/ --watch",
    "dev:playground": "cross-env vite serve playground",
    "build:esm": "cross-env swc src --out-dir dist/esm/",
    "build:cjs": "cross-env swc -C module.type=commonjs src --out-dir dist/cjs/",
    "watch": "",
    "playground": "vite serve playground"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.39",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.1.0",
    "vite": "^1.0.0-rc.9",
    "vue": "^3.0.0"
  }
}
